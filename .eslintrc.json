{
  "env": {
    "es2020": true
  },

  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    // "prettier",
    // "prettier/react",
    // "prettier/legacy",
    // "prettier/react-legacy"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "import"],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"],
        "sort-imports": "error"
      }
    ]
  }
}
