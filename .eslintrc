{
  "env": {
    "es2020": true
  },

  "extends": [
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "import"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [
      "warn",
      {"extensions": [".js", ".jsx"]}
    ],
    "import/prefer-default-export": "off",
    "jsx-quotes": ["error", "prefer-single"]
  }
}
